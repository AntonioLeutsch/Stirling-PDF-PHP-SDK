{
    "name": "antonioleutsch/stirling-pdf-sdk",
    "description": "PHP SDK for the Stirling PDF API - PDF processing, conversion, manipulation, security and more.",
    "type": "library",
    "keywords": [
        "stirling-pdf",
        "pdf",
        "php",
        "sdk",
        "api",
        "pdf-conversion",
        "pdf-manipulation"
    ],
    "homepage": "https://github.com/AntonioLeutsch/Stirling-PDF-PHP-SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Leutsch",
            "homepage": "https://github.com/AntonioLeutsch"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
